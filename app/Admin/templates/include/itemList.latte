{if $items}

    {foreach $items as $item}

        <div>

            {if $item->isActive === false}

                <a n:href="detail, id => $item->id">{$item->name}</a>

            {elseif $item->isActive && $item->drafts->count()}

                <a n:href="WikiDraft:default, wikiId => $item->id">{$item->name}</a>

            {else}

                <a n:href="form, id => $item->id">{$item->name}</a>

            {/if}

        </div>

    {/foreach}

{/if}
